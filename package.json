{
  "name": "the-last-redoubt",
  "version": "1.0.0",
  "private": true,
  "description": "the-last-redoubt",
  "author": "Rob Myers",
  "keywords": [
    "gatsby"
  ],
  "browserslist": [
    ">0.25%",
    "not dead and supports es6-module"
  ],
  "scripts": {
    "develop": "gatsby develop -p 8010 -H 0.0.0.0",
    "dev": "concurrently 'yarn develop' 'yarn watch-svg-meta'",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "lint": "eslint . --ext .js,.jsx,.tsx,.ts",
    "typecheck": "tsc --noEmit --incremental true",
    "types": "yarn typecheck",
    "postinstall": "patch-package",
    "minify-pngs": "yarn ts-script src/scripts/minify-pngs",
    "rename-pngs": "yarn ts-script src/scripts/rename-pngs",
    "bake-lighting": "yarn ts-script src/scripts/bake-lighting",
    "render-layout": "yarn ts-script src/scripts/render-layout",
    "render-npc": "yarn ts-script src/scripts/render-npc",
    "npcs-meta": "yarn ts-script src/scripts/npcs-meta",
    "pngs-to-webp": "yarn ts-script src/scripts/pngs-to-webp",
    "process-sheets": "yarn ts-script src/scripts/process-sheets",
    "simplify-pngs": "yarn ts-script src/scripts/simplify-pngs",
    "svg-meta": "yarn ts-script src/scripts/svg-meta",
    "ts-script": "NODE_OPTIONS=\"--no-experimental-fetch\" ts-node -O '{ \"module\": \"commonjs\", \"isolatedModules\": false }'",
    "watch-svg-meta": "nodemon --delay 2s --config src/scripts/svg-meta.nodemon.json --exec 'yarn svg-meta'"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "@emotion/react": "^11.9.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.8.1",
    "@giscus/react": "^2.2.6",
    "@loadable/component": "^5.15.2",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "@pixi/react": "^7.1.1",
    "@react-three/drei": "^9.88.7",
    "@react-three/fiber": "^8.15.8",
    "@types/three": "^0.158.0",
    "body-scroll-lock": "^4.0.0-beta.0",
    "braces": "^3.0.2",
    "canvas": "^2.9.3",
    "cheerio": "^1.0.0-rc.10",
    "cli-columns": "^4.0.0",
    "debounce": "^1.2.1",
    "earcut": "^2.2.3",
    "flexlayout-react": "^0.7.3",
    "gatsby": "^5.3.3",
    "gatsby-plugin-emotion": "^8.2.0",
    "gatsby-plugin-gatsby-cloud": "^5.3.1",
    "gatsby-plugin-loadable-components-ssr": "^4.3.2",
    "gatsby-plugin-mdx": "^5.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-remark-katex": "^7.2.0",
    "gatsby-source-filesystem": "^5.2.0",
    "getopts": "^2.3.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "jsx-dom-cjs": "^8.0.1-beta.8",
    "katex": "^0.16.3",
    "lodash.clonedeep": "^4.5.0",
    "mvdan-sh": "^0.5.0",
    "pixi-viewport": "^5.0.2",
    "pixi.js": "^7.3.2",
    "poly2tri": "^1.5.0",
    "polygon-clipping": "^0.15.3",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
