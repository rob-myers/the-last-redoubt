    "@types/mdx": "^2.0.3",
    "@types/node": "^17.0.31",
    "@types/prismjs": "^1.26.0",
    "@types/react": "^18.0.9",
    "@types/react-beforeunload": "^2.1.1",
    "@types/react-dom": "^17.0.16",
    "@types/react-helmet": "^6.1.5",
    "@types/svg-path-parser": "^1.1.3",
    "@types/tiny-async-pool": "^2.0.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.4",
    "@types/xml2js": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "babel-preset-gatsby": "^2.19.0",
    "concurrently": "^6.0.1",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gatsby-plugin-tsconfig-paths": "^1.0.5",
    "gatsby-plugin-webpack-bundle-analyser-v2": "^1.1.30",
    "nodemon": "^2.0.7",
    "patch-package": "^6.4.7",
    "ts-node": "^10.0.0",
    "typescript": "^5.1.6",
    "xml2js": "^0.4.23"
  }
}
