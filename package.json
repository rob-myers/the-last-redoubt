    "cli-columns": "^4.0.0",
    "debounce": "^1.2.1",
    "earcut": "^2.2.3",
    "flexlayout-react": "^0.7.3",
    "gatsby": "^4.19.2",
    "gatsby-plugin-emotion": "^7.13.0",
    "gatsby-plugin-loadable-components-ssr": "^4.2.0",
    "gatsby-plugin-mdx": "2.14.2",
    "gatsby-plugin-react-helmet": "^5.14.0",
    "gatsby-source-filesystem": "^4.13.0",
    "getopts": "^2.3.0",
    "json-stringify-pretty-compact": "^3.0.0",
    "jsx-dom-cjs": "^8.0.1-beta.8",
    "katex": "^0.16.2",
