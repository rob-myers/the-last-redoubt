    "polygon-clipping": "^0.15.3",
    "prism-react-renderer": "^1.3.5",
    "prismjs": "^1.28.0",
    "react": "^18.2.0",
    "react-beforeunload": "^2.5.3",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-query": "^3.39.0",
    "react-reverse-portal": "^2.1.0",
    "react-sizeme": "^3.0.2",
    "react-use-measure": "^2.1.1",
