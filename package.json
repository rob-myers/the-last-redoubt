    "react-helmet": "^6.1.0",
    "react-lite-youtube-embed": "^2.3.52",
    "react-query": "^3.39.3",
    "react-sizeme": "^3.0.2",
    "react-use-measure": "^2.1.1",
    "rehype-katex": "^4",
    "remark-gfm": "^1",
    "remark-math": "^3",
    "rxjs": "7.0.0-beta.10",
    "safe-stable-stringify": "^2.3.1",
    "svg-path-parser": "^1.1.0",
    "swiper": "^8.3.2",
    "three": "^0.158.0",
    "tiny-async-pool": "^1.2.0",
    "triangle-wasm": "^1.0.0",
    "uid": "^2.0.0",
    "xterm": "^4.18.0",
    "xterm-addon-fit": "^0.5.0",
    "zustand": "^4.1.1"
